{
  "name": "notes-viewer",
  "displayName": "Notes Viewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscodeSidebar.openview",
    "onView:vscodeSidebar.openview"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "notes-viewer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscodeSidebar.openview",
        "title": "Sidebar View"
      },
      {
        "command": "vscodeSidebar.menu.view",
        "title": "Sample WebView in VS Code Sidebar"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "vscodeSidebar.menu.view",
          "group": "navigation",
          "when": "view == vscodeSidebar.openview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "html-loader": "^5.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "markdown": "^0.5.0",
    "markdown-it": "^14.1.0",
    "marked": "^15.0.7"
  }
}
